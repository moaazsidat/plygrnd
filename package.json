{
  "name": "plygrnd",
  "version": "1.0.1",
  "description": "Playground for testing NPM publishing with Github Actions",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moaazsidat/plygrnd.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/moaazsidat/plygrnd/issues"
  },
  "homepage": "https://github.com/moaazsidat/plygrnd#readme",
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      ["@semantic-release/git", {
        "assets": ["dist/**/*.{js,css}", "docs", "package.json"],
        "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
      }]
    ]
  },
  "devDependencies": {
    "@semantic-release/git": "^9.0.0"
  }
}
